<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for item in list %}
    <url>
        <loc>{{ item.url }}</loc>
        <lastmod>{{ item.date }}</lastmod>
        <changefreq>{{ item.changefreq }}</changefreq>
        <priority>1</priority>
    </url>
{% endfor %}
</urlset>
